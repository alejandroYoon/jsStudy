import react, { useState } from "react";

const State = () => {
  const [count, setCount] = useState(0);

  // const increment = () => {
  //   setCount(count + 1);
  // };

  const increment = () => {
    setCount (count + 1);
  };

  return (
    <div>
      <p>Count: {count}</p>
      <button onClick={increment}>Increment123</button>
    </div>
  );
};

export default State